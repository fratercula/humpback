{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": [2, "never"],
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "react/state-in-constructor": 0,
    "no-throw-literal": 0,
    "react/jsx-filename-extension": 0
  }
}
